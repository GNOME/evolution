msgstr "Afficher comme pièce jointe"

#: ../em-format/e-mail-formatter-headers.c:107
#: ../em-format/e-mail-part-headers.c:47 ../mail/e-mail-tag-editor.c:272
#: ../mail/message-list.etspec.h:5 ../modules/mail/em-mailer-prefs.c:64
msgid "From"
msgstr "De"

#: ../em-format/e-mail-formatter-headers.c:133
msgid "(no subject)"
msgstr "(pas de sujet)"

#: ../em-format/e-mail-formatter-headers.c:333
#, c-format
msgid "This message was sent by %s on behalf of %s"
msgstr "Ce message a été envoyé par %s pour le compte de %s"

# Titre de fenêtre
#: ../em-format/e-mail-formatter-image.c:152
