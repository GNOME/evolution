msgid "_Do not save"
msgstr "_Ne shrani"

#: ../addressbook/addressbook.error.xml.h:41
msgid "_Resize"
msgstr "_Povečaj"

#: ../addressbook/addressbook.error.xml.h:42
msgid "_Use as it is"
msgstr "_Uporabi kot je"

