    <item action='address-book-delete-popup'/>
    <separator/>
    <placeholder id='address-book-popup-actions'/>
    <separator/>
    <item action='address-book-manage-groups-popup'/>
    <separator/>
    <item action='address-book-map-popup'/>
    <separator/>
    <item action='address-book-properties-popup'/>
