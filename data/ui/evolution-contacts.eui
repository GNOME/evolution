          <submenu action='contact-cards-sort-by-menu'>
            <item action='contact-cards-sort-by-file-as' group='contact-cards-sort'/>
            <item action='contact-cards-sort-by-given-name' group='contact-cards-sort'/>
            <item action='contact-cards-sort-by-family-name' group='contact-cards-sort'/>
          </submenu>
        </placeholder>
      </submenu>
      <placeholder id='view-custom-menus'>
        <submenu action='contact-preview-menu'>
          <item action='contact-preview'/>
