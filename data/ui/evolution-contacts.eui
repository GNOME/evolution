  <toolbar id='main-toolbar-without-headerbar'>
    <item action='address-book-print'/>
    <item action='delete-selection'/>
    <item action='address-book-stop'/>
  </toolbar>
  <menu id='address-book-popup' is-popup='true'>
    <item action='address-book-new'/>
