      </placeholder>
    </submenu>
    <placeholder id='custom-menus'>
      <submenu action='contact-actions-menu'>
        <item action='contact-forward'/>
        <item action='contact-send-message'/>
        <item action='address-book-stop'/>
        <separator/>
        <item action='contact-copy'/>
        <item action='contact-move'/>
        <separator/>
        <item action='contact-bulk-edit'/>
        <separator/>
        <item action='address-book-copy'/>
        <item action='address-book-move'/>
        <separator/>
        <item action='address-book-map'/>
        <separator/>
        <item action='address-book-properties'/>
      </submenu>
    </placeholder>
