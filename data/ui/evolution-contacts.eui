<eui>
  <menu id='main-menu'>
    <submenu action='file-menu'>
      <placeholder id='file-actions'>
        <item action='contact-open'/>
        <item action='contact-save-as'/>
        <item action='address-book-save-as'/>
      </placeholder>
      <placeholder id='print-actions'>
        <item action='address-book-print-preview'/>
        <item action='address-book-print'/>
      </placeholder>
    </submenu>
    <submenu action='edit-menu'>
      <placeholder id='edit-actions'>
        <item action='contact-delete'/>
        <item action='address-book-delete'/>
